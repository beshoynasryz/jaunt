<main id="main" class="main">

    <div class="pagetitle">
      <h1>Juant Company</h1>
      <nav>
        <ol class="breadcrumb">
          <li class="breadcrumb-item"><a href="index.html">Home</a></li>
          <li class="breadcrumb-item"><a href="partners.html"></a> Partners</li>
          <li class="breadcrumb-item active">Partner Details</li>
        </ol>
      </nav>
    </div><!-- End Page Title -->

    <section class="section places">
      <div class="row">

        





        <section class="section profile">
          <div class="row">
    
            <div class="col-xl-12">
    
              <div class="card">
                <div class="card-body pt-3">
                  
                  
                    <div class="back-bar text-center">
                        <a href="partners.html"><i class="bi bi-arrow-left-circle-fill"></i>                   
                        </a>
                      </div><!-- End Search Bar -->


                  <div class="tab-content pt-2">

        <!-- Left side columns -->
        <div class="col-lg-12">
          <div class="row">



                <div class="add-places">
                    <div class="card-body">
                        <div class="col-lg-12 justify-content-center">
                            <div class="row">
                              <h5 class="card-title text-center">Partner Details</h5>
                  
                  
                              <div class="col-lg-6 mb-3">
                                  <div class="row">
                                      <!-- Single Price Plan Area-->
                                      <div class="mt-4 text-center">
                                          <div class="card-ad text-center">
                                          <div class="single_price_plan active wow fadeInUp" data-wow-delay="0.2s" style="visibility: visible; animation-delay: 0.2s; animation-name: fadeInUp;">
                                            <!-- Side Shape-->
                                         
                  
                                            <div class="card-icon text-center">
                                              <img src="<%=ownerdetials.image%>" alt="">
                                            </div>
                                            <div class="title pb-2 pt-2">
                                              <h3><%=ownerdetials.ownername%></h3>
                                          </div>
                                        </div>
                                      </div>
                                  </div>
                              </div>
                              </div>
                  
                  
                              <div class="col-lg-6 mt-2">
                                  <div class="row">
                                          <div class="news">
                                            <div class="row g-2">
                
                            
                                              <div class="row mt-3">
                                                <div class="col-lg-4 col-md-4 label"><i class="bi bi-building"></i> Company Name:</div>
                                                <div class="col-lg-8 col-md-8"><%=ownerdetials.companyname%></div>
                                              </div>
                            
                                              <div class="row">
                                                <div class="col-lg-4 col-md-4 label"><i class="bi bi-bookmark"></i> Company Type</div>
                                                <div class="col-lg-8 col-md-8"><%=ownerdetials.type%></div>
                                              </div>

                                              <div class="row">
                                                <div class="col-lg-4 col-md-4 label"><i class="bi bi-buildings"></i> Branches</div>
                                                <div class="col-lg-8 col-md-8">6</div>
                                              </div>
                  
                                              <div class="row">
                                                <div class="col-lg-4 col-md-4 label"><i class="bi bi-telephone"></i> Phone</div>
                                                <div class="col-lg-8 col-md-8"><%=ownerdetials.phone%></div>
                                              </div>
                            
                                              <div class="row">
                                                <div class="col-lg-4 col-md-4 label"><i class="bi bi-envelope"></i> Email</div>
                                                <div class="col-lg-8 col-md-8"><%=ownerdetials.email%></div>
                                              </div>
                            
                            
                            
                                          </div><!-- End sidebar recent posts-->
                            
                                        </div>
                                  </div>
                              </div>
                              
                        </div>
                      </div>
                    </div>
                  </div>
      


                  <h5 class="card-title text-center">Activated Branches</h5>





            
            <!-- place Card -->
            <% placesowner.forEach(function(place){ %>
            <div class="col-xxl-4 col-md-4 my-4">
                <div class="card info-card sales-card">
  
                  <div class="filter">
                    <a class="icon" href="#" data-bs-toggle="dropdown"><i class="bi bi-three-dots"></i></a>
                    <ul class="dropdown-menu dropdown-menu-end dropdown-menu-arrow">
                      <li><a class="dropdown-item" href="Branch-Detils.html">See Details</a></li>
                      <li><a class="dropdown-item" href="edit-places.html">Delete</a></li>
                    </ul>
                  </div>
  
                  <div class="card-body my-4 pb-1 text-center align-items-center">
                    <div class="card-icon d-flex align-items-center justify-content-center">
                      <img src="<%= place.images[0] %>" alt="">
                </div>
                    
                    <p class="pt-4"><%=place.desc%></p>
  
                      <div class="pt-2 pb-2">
                        <span class="small pt-1 fw-bold bi bi-geo-alt"></span> <span class="text-muted small pt-1 ps-1"><%=place.city%> , <%=place.area%></span>
                        <!-- <span class="small pt-1 fw-bold bi bi-geo-alt"></span> <span class="text-muted small pt-1 ps-1"><%=place.area%></span> -->
                      </div>
                      <!-- <span class="small pt-1 fw-bold bi bi-upc"></span> <span class="text-muted small pt-1 ps-1">ID: 1234654afsdgv25</span> -->

                  </div>
  
                </div>
              </div>
              <% }); %>
              <!-- End place Card -->

            

                        

          </div>
        </div><!-- End Left side columns -->
                  </div>
                </div>
              </div>
            </div>
          </div>
        </section>
























      </div>
    </section>

  </main>